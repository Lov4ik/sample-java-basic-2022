public class Main {
    public static void main(String[] args) {

        Bank bank = new Bank();
        Client client1 = new Client("Петров", 31);
        Client client2 = new Client("Фалиндричев", 30);
        Client client3 = new Client("Пупкин", 33);
        Client client4 = new Client("Ловчиков", 28);
        bank.addNewClient(client1);
        bank.addNewClient(client2);
        bank.addNewClient(client3);
        Account account1 = new Account(1,123.1);
        Account account2 = new Account(2, 133.1);
        Account account3 = new Account(3,223.1);
        Account account4 = new Account(4, 100.3);

        client1.addAccount(account1);
        client1.addAccount(account2);
        client2.addAccount(account3);
        bank.getAccounts(client2);
        client2.printResult();
        bank.printClient(bank.findClient(account2));
    }
}
